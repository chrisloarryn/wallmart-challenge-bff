{
  "name": "back_geekz_products",
  "version": "1.2.0",
  "description": "Microservicio de productos de Geekz",
  "repository": {
    "type": "git",
    "url": "git@geekz.github.com:geekzdev/back_geekz_products.git"
  },
  "license": "MIT",
  "author": "Nicolas Galdames",
  "main": "app.js",
  "scripts": {
    "build": "tsc",
    "build-ts": "tsc",
    "build:dev": "nodemon \"src/app.ts\" --exec \"ts-node\" src/app.ts -e ts,json,gql",
    "build:old": "npm run clean && tsc -p . && ncp src/schema dist/schema",
    "clean": "tsc --build --clean",
    "clean:old": "rimraf ./dist",
    "commit": "cz",
    "coverage": "jest --coverage",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "release": "standard-version",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "start": "node dist/app.js",
    "start:dev": "npm run build:dev",
    "watch-ts": "tsc -w"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@types/express-fileupload": "^1.1.6",
    "@types/uuid": "^8.3.0",
    "@validatecl/type": "^1.0.5",
    "apollo-server-express": "^2.25.1",
    "axios": "^0.21.1",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "dayjs": "^1.10.7",
    "graphql": "^15.5.0",
    "graphql-import-node": "0.0.4",
    "graphql-playground-middleware-express": "^1.7.22",
    "graphql-request": "^3.5.0",
    "graphql-tools": "^7.0.5",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^13.0.0",
    "http": "0.0.1-security",
    "lodash": "^4.17.21",
    "ncp": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.29.3",
    "stream": "^0.0.2",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.3",
    "type-graphql": "^1.1.1",
    "typedi": "^0.10.0",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@sentry/node": "^6.13.2",
    "@sentry/tracing": "^6.13.2",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/express-graphql": "^0.9.0",
    "@types/graphql": "^14.5.0",
    "@types/graphql-upload": "^8.0.7",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.170",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^15.14.9",
    "@types/sharp": "^0.29.4",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "bcrypt": "^5.0.1",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "cz-conventional-changelog": "^3.0.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "express": "^4.17.2",
    "express-fileupload": "^1.2.1",
    "express-validator": "^6.11.1",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1",
    "standard-version": "^9.3.1"
  }
}
